{
  "name": "Ender Umbreon",
  "description": "You come from unnatrual places. Giving you unwanted debuffs.",
  "type": "origins:multiple",
  "light_sensitive": {
    "type": "origins:shader",
    "shader": "minecraft:shaders/post/phosphor.json",
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:block_in_radius",
              "radius": 1,
              "shape": "cube",
              "block_condition": {
                "type": "origins:light_level",
                "light_type": "block",
                "compare_to": 11,
                "comparison": ">="
              }
            },
            {
              "type": "origins:status_effect",
              "effect": "minecraft:glowing",
              "inverted": true
            }
          ]
        },
        {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:daytime"
            },
            {
              "type": "origins:exposed_to_sky"
            },
            {
              "type": "origins:status_effect",
              "effect": "minecraft:glowing",
              "inverted": true
            }
          ]
        }
      ]
    }
  },
  "light_weakness": {
    "type": "origins:action_over_time",
    "interval": 10,
    "entity_action": {
      "type": "origins:apply_effect",
      "effects": [
        {
          "duration": 40,
          "amplifier": 9,
          "effect": "minecraft:weakness",
          "show_icon": true,
          "show_particles": false
        },
        {
          "duration": 40,
          "amplifier": 3,
          "effect": "minecraft:mining_fatigue",
          "show_icon": true,
          "show_particles": false
        }
      ]
    },
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:block_in_radius",
              "radius": 1,
              "shape": "cube",
              "block_condition": {
                "type": "origins:light_level",
                "light_type": "block",
                "compare_to": 11,
                "comparison": ">="
              }
            },
            {
              "type": "origins:status_effect",
              "effect": "minecraft:glowing",
              "inverted": true
            }
          ]
        },
        {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:daytime"
            },
            {
              "type": "origins:exposed_to_sky"
            },
            {
              "type": "origins:status_effect",
              "effect": "minecraft:glowing",
              "inverted": true
            }
          ]
        }
      ]
    }
  },
  "endermen_nice": {
    "type": "apugli:mobs_ignore",
    "mob_condition": {
      "type": "origins:entity_type",
      "entity_type": "minecraft:enderman"
    }
  },
  "reach": {
    "type": "origins:attribute",
    "modifiers": [
      {
        "name": "Extra Reach block reach",
        "attribute": "reach-entity-attributes:reach",
        "value": 2,
        "operation": "addition"
      },
      {
        "name": "Extra Reach entity reach",
        "attribute": "reach-entity-attributes:attack_range",
        "value": 0.5,
        "operation": "addition"
      }
    ]
  },
  "height": {
    "type": "origins:action_on_callback",
    "execute_chosen_when_orb": true,
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height .9 @s",
      "permission_level": 4
    },
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height .9 @s",
      "permission_level": 4
    },
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height .9 @s",
      "permission_level": 4
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1 @s",
      "permission_level": 4
    },
    "entity_action_removed": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1 @s",
      "permission_level": 4
    }
  },
  "width": {
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width .9 @s",
      "permission_level": 4
    },
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width .9 @s",
      "permission_level": 4
    },
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width .9 @s",
      "permission_level": 4
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1 @s",
      "permission_level": 4
    },
    "entity_action_removed": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1 @s",
      "permission_level": 4
    }
  },
  "water_damage": {
    "type": "origins:damage_over_time",
    "interval": 20,
    "onset_delay": 40,
    "damage": 20,
    "damage_easy": 10,
    "damage_source": {
      "name": "hurt_by_water",
      "unblockable": true,
      "bypasses_armor": true
    },
    "protection_enchantment": "origins:water_protection",
    "protection_effectiveness": 1,
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": 0.1
        },
        {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:in_rain"
            },
            {
              "type": "origins:in_block",
              "block_condition": {
                "type": "origins:height",
                "compare_to": 220,
                "comparison": "<="
              }
            }
          ]
        },
        {
          "type": "origins:in_block",
          "block_condition": {
            "type": "origins:block_state",
            "property": "level",
            "compare_to": 1,
            "comparison": ">="
          }
        }
      ]
    }
  },
  "silk_hands": {
    "type": "origins:modify_harvest",
    "allow": true,
    "block_condition": {
      "type": "origins:in_tag",
      "tag": "ashley:ashley/silk_hands"
    },
    "condition": {
      "type": "origins:equipped_item",
      "equipment_slot": "mainhand",
      "item_condition": {
        "type": "origins:ingredient",
        "ingredient": {
          "item": "minecraft:air"
        }
      }
    }
  },
  "end_sight": {
    "type": "origins:night_vision",
    "strength": 0.7,
    "condition": {
      "type": "origins:dimension",
      "dimension": "minecraft:the_end"
    }
  },
  "particles": {
    "type": "origins:action_over_time",
    "interval": 5,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "particle minecraft:dragon_breath ~ ~ ~ 0.5 0.9 0.5 .01 5 normal"
    },
    "condition": { "type": "origins:sneaking", "inverted": true }
  },
  "xp": {
    "type": "origins:modify_xp_gain",
    "modifier": {
      "operation": "multiply_base",
      "value": 3.5
    }
  },
  "glow_berry_death": {
    "type": "origins:modify_food",
    "always_edible": false,
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": {
        "item": "minecraft:glow_berries"
      }
    },
    "entity_action": {
      "type": "origins:execute_command",
      "command": "kill @s"
    },
    "condition": {
      "type": "origins:status_effect",
      "effect": "minecraft:glowing",
      "inverted": true
    }
  },
  "speed": {
    "type": "origins:attribute",
    "modifier": {
      "attribute": "minecraft:generic.movement_speed",
      "operation": "addition",
      "value": 0.02
    }
  },
  "chorus_flowers": {
    "type": "apugli:edible_item",
    "food_component": {
      "always_edible": true,
      "meat": false,
      "snack": false,
      "hunger": 10,
      "saturation": 1.7
    },
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": { "item": "minecraft:chorus_flower" }
    },
    "entity_action": {
      "type": "origins:apply_effect",
      "effects": [
        {
          "effect": "minecraft:regeneration",
          "duration": 100,
          "show_particles": true,
          "show_icon": true
        },
        {
          "effect": "minecraft:glowing",
          "duration": 3000,
          "show_particles": false,
          "show_icon": true
        }
      ]
    }
  },
  "popped_chorus": {
    "type": "apugli:edible_item",
    "food_component": {
      "always_edible": false,
      "meat": false,
      "snack": true,
      "hunger": 6,
      "saturation": 1
    },
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": { "item": "minecraft:popped_chorus_fruit" }
    }
  },
  "reduced_crystal": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "operation": "add_base_late",
      "value": -10,
      "name": "end_crystal"
    },
    "damage_condition": {
      "type": "origins:name",
      "name": "explosion"
    },
    "condition": {
      "type": "origins:block_in_radius",
      "compare_to": 1,
      "comparison": ">=",
      "radius": 6,
      "shape": "cube",
      "block_condition": {
        "type": "origins:in_tag",
        "tag": "ashley:ashley/crystal_placable"
      }
    }
  },
  "reduced_crystal_player": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "operation": "add_base_late",
      "value": -10,
      "name": "end_crystal"
    },
    "damage_condition": {
      "type": "origins:name",
      "name": "explosion.player"
    },
    "condition": {
      "type": "origins:block_in_radius",
      "compare_to": 1,
      "comparison": ">=",
      "radius": 6,
      "shape": "cube",
      "block_condition": {
        "type": "origins:in_tag",
        "tag": "ashley:ashley/crystal_placable"
      }
    }
  },
  "healing_crystals": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:apply_effect",
      "effects": [
        {
          "effect": "minecraft:absorption",
          "duration": 100,
          "amplifier": 1
        },
        {
          "effect": "minecraft:regeneration",
          "duration": 100,
          "amplifier": 1
        }
      ]
    },
    "condition": {
      "type": "moborigins:nearby_entities",
      "compare_to": 1,
      "comparison": ">=",
      "multiplier": 6,
      "bientity_condition": {
        "type": "origins:target_condition",
        "condition": {
          "type": "origins:entity_type",
          "entity_type": "minecraft:end_crystal"
        }
      }
    }
  }
}
