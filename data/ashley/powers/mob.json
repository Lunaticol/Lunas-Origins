{
  "type": "origins:multiple",
  "kill": {
    "type": "origins:action_over_time",
    "interval": 2000,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "kill @s"
    }
  },
  "ignore": {
    "type": "origins:action_on_hit",
    "bientity_action": {
      "type": "origins:actor_action",
      "action": {
        "type": "origins:execute_command",
        "command": "kill @s"
      }
    },
    "bientity_condition": {
      "type": "origins:target_condition",
      "condition": {
        "type": "origins:origin",
        "layer": "origins:origin",
        "origin": "ashley:ashley",
        "inverted": true
      }
    }
  }
}
