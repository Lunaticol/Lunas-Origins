{
  "type": "origins:multiple",
  "name": "",
  "description": "",
  "hidden": true,
  "turbo_engine": {
    "type": "origins:resource",
    "max": 1,
    "min": 0
  },
  "titan_kit": {
    "type": "origins:action_on_callback",
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "origin gui @s scorch:titan_kits"
    }
  },
  "kit": {
    "type": "origins:action_on_callback",
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "origin gui @s scorch:kits"
    }
  },
  "food": {
    "type": "origins:modify_exhaustion",
    "modifier": {
      "name": "food",
      "operation": "multiply_total",
      "value": -2
    }
  },
  "titan_battery_eat": {
    "type": "apugli:edible_item",
    "food_component": {
      "always_edible": true,
      "snack": false,
      "meat": false,
      "saturation": 2,
      "hunger": 20
    },
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:heal",
          "amount": 8
        },
        {
          "type": "origins:change_resource",
          "change": 25,
          "operation": "add",
          "resource": "*:core_core_bar"
        },
        {
          "type": "origins:change_resource",
          "change": 1,
          "operation": "set",
          "resource": "*:*_battery_cooldown"
        }
      ]
    },
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": {
        "item": "minecraft:redstone_block"
      }
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:health",
          "compare_to": 60,
          "comparison": "<="
        },
        {
          "type": "origins:resource",
          "compare_to": 0,
          "comparison": "==",
          "resource": "*:*_battery_cooldown"
        }
      ]
    }
  },
  "death_warning": {
    "type": "origins:action_over_time",
    "interval": 2,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "particle minecraft:smoke ~ ~ ~ 0.5 0.5 0.5 .01 20 force"
        },
        {
          "type": "origins:execute_command",
          "command": "particle minecraft:lava ~ ~ ~ 0.5 0.5 0.5 1 1 force"
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:health",
          "comparison": "<=",
          "compare_to": 20
        }
      ]
    }
  },
  "sprint": {
    "type": "origins:prevent_sprinting"
  },
  "battery_cooldown": {
    "type": "origins:resource",
    "max": 1,
    "min": 0
  },
  "battery_recharge": {
    "type": "origins:action_over_time",
    "interval": 100,
    "entity_action": {
      "type": "origins:change_resource",
      "change": 0,
      "operation": "set",
      "resource": "*:*_battery_cooldown"
    },
    "condition": {
      "type": "origins:resource",
      "compare_to": 1,
      "comparison": "==",
      "resource": "*:*_battery_cooldown"
    }
  },
  "health_low": {
    "type": "origins:action_over_time",
    "interval": 5,
    "entity_action": {
      "type": "origins:if_else_list",
      "actions": [
        {
          "action": {
            "type": "origins:execute_command",
            "command": "particle minecraft:campfire_signal_smoke ~ ~1 ~ 0.5 1 0.5 0 1 force"
          },
          "condition": {
            "type": "origins:and",
            "conditions": [
              {
                "type": "origins:health",
                "compare_to": 40,
                "comparison": "<="
              },
              {
                "type": "origins:health",
                "compare_to": 31,
                "comparison": ">="
              }
            ]
          }
        },
        {
          "action": {
            "type": "origins:execute_command",
            "command": "particle minecraft:campfire_signal_smoke ~ ~1 ~ 1 1 1 0.01 5 force"
          },
          "condition": {
            "type": "origins:health",
            "compare_to": 30,
            "comparison": "<="
          }
        }
      ]
    }
  },
  "change_origin": {
    "type": "origins:action_on_callback",
    "entity_action_lost": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "origin set @s scorch:kits origins:empty"
        },
        {
          "type": "origins:execute_command",
          "command": "origin set @s scorch:titan_kits origins:empty"
        }
      ]
    }
  },
  "powers": {
    "type": "origins:action_on_callback",
    "entity_action_gained": {
      "type": "origins:execute_command",
      "command": "function scorch:powers"
    },
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "function scorch:powers"
    }
  }
}
