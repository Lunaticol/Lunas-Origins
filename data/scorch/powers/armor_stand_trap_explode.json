{
  "type": "origins:multiple",
  "fire": {
    "type": "origins:action_over_time",
    "interval": 5,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "particle minecraft:falling_dust red_terracotta ~ ~1 ~ 3 0 3 1 100 force"
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "inverted": true,
          "type": "origins:command",
          "command": "flan claimInfo",
          "comparison": "==",
          "compare_to": 1
        },
        {
          "type": "origins:resource",
          "compare_to": 0,
          "comparison": "==",
          "resource": "*:*_kill_armorstan_trap_bar"
        },
        {
          "type": "origins:resource",
          "compare_to": 0,
          "comparison": "==",
          "resource": "*:*_on_fire_bar"
        }
      ]
    }
  },
  "kill_armorstan_trap_bar": {
    "type": "origins:resource",
    "max": 1,
    "min": 0
  },
  "on_fire_bar": {
    "type": "origins:resource",
    "max": 1,
    "min": 0
  },
  "kill_armorstand_trap": {
    "type": "origins:action_over_time",
    "interval": 200,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:change_resource",
          "change": 1,
          "resource": "*:*_kill_armorstan_trap_bar"
        },
        {
          "type": "origins:execute_command",
          "command": "kill @s"
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "compare_to": 0,
          "comparison": "==",
          "resource": "*:*_kill_armorstan_trap_bar"
        },
        {
          "type": "origins:resource",
          "compare_to": 0,
          "comparison": "==",
          "resource": "*:*_on_fire_bar"
        }
      ]
    }
  },
  "ignite": {
    "type": "origins:action_over_time",
    "interval": 10,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:change_resource",
          "change": 1,
          "resource": "*:*_on_fire_bar"
        },
        {
          "type": "origins:explode",
          "power": 2,
          "damage_self": false,
          "destruction_type": "none"
        },
        {
          "type": "origins:area_of_effect",
          "radius": 6,
          "shape": "cube",
          "bientity_action": {
            "type": "origins:target_action",
            "action": {
              "type": "origins:damage",
              "amount": 5,
              "damage_type": "minecraft:lava",
              "source": {
                "fire": true,
                "name": "Explode"
              }
            }
          }
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:block_in_radius",
          "block_condition": {
            "type": "origins:or",
            "conditions": [
              { "type": "origins:block", "block": "minecraft:fire" },
              { "type": "origins:block", "block": "minecraft:soul_fire" }
            ]
          },
          "shape": "cube",
          "radius": 6
        },
        {
          "inverted": true,
          "type": "origins:command",
          "command": "flan claimInfo",
          "comparison": "==",
          "compare_to": 1
        }
      ]
    }
  },
  "fire_ignited": {
    "type": "origins:action_over_time",
    "interval": 5,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:and",
          "actions": [
            {
              "type": "origins:execute_command",
              "command": "particle minecraft:flame ~ ~1 ~ 3 0 3 0 100 force"
            },
            {
              "type": "origins:execute_command",
              "command": "particle minecraft:lava ~ ~ ~ 1 4 1 0.01 20 force"
            }
          ]
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "inverted": true,
          "type": "origins:command",
          "command": "flan claimInfo",
          "comparison": "==",
          "compare_to": 1
        },
        {
          "type": "origins:resource",
          "compare_to": 0,
          "comparison": "==",
          "resource": "*:*_kill_armorstan_trap_bar"
        },
        {
          "type": "origins:resource",
          "compare_to": 1,
          "comparison": "==",
          "resource": "*:*_on_fire_bar"
        }
      ]
    }
  },
  "kill_armorstan_ignited": {
    "type": "origins:action_over_time",
    "interval": 200,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:change_resource",
          "change": 1,
          "resource": "*:*_kill_armorstan_trap_bar"
        },
        {
          "type": "origins:execute_command",
          "command": "kill @s"
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "compare_to": 0,
          "comparison": "==",
          "resource": "*:*_kill_armorstan_trap_bar"
        },
        {
          "type": "origins:resource",
          "compare_to": 1,
          "comparison": "==",
          "resource": "*:*_on_fire_bar"
        }
      ]
    }
  }
}
