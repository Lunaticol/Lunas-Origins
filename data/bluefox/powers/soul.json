  "soul_siphon": {
    "type": "origins:self_action_on_hit",
    "entity_action": {
      "type": "origins:change_resource",
      "change": 15,
      "operation": "add",
      "resource": "*:*_soul_bar"
    }
  },
  "on_soulsand": {
    "type": "origins:action_over_time",
    "interval": 15,
    "entity_action": {
      "type": "origins:change_resource",
      "change": 25,
      "operation": "add",
      "resource": "*:*_soul_bar"
    },
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:block_in_radius",
          "compare_to": 1,
          "comparison": ">=",
          "radius": 2,
          "shape": "cube",
          "block_condition": {
            "type": "origins:in_tag",
            "tag": "bluefox:blocks"
          }
        }
      ]
    }
  },
  "soul_weapons": {
    "type": "origins:conditioned_attribute",
    "tick_rate": 5,
    "modifier": {
      "attribute": "minecraft:generic.attack_damage",
      "value": 0.3,
      "operation": "multiply_base"
    },
    "condition": {
      "type": "origins:equipped_item",
      "equipment_slot": "mainhand",
      "item_condition": {
        "type": "origins:ingredient",
        "ingredient": {
          "tag": "bluefox:soul_weapons"
        }
      }
    }
  },
  "soul_tooltip": {
    "type": "origins:tooltip",
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": {
        "tag": "bluefox:soul_weapons"
      }
    },
    "texts": [
      {
        "text": "Aono Bonus:",
        "color": "gray"
      },
      {
        "text": "+30% Attack Damage",
        "color": "blue"
      }
    ]
  }
}
