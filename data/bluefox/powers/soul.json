{
  "name": "Spiritual Communication",
  "description": "You are able to absorb souls into yourself. This provides many benefits for you.",
  "type": "origins:multiple",
  "soul_bar": {
    "type": "origins:resource",
    "max": 1000,
    "min": 0,
    "hud_render": {
      "should_render": true,
      "bar_index": 15,
      "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png"
    }
  },
  "particles": {
    "type": "origins:self_action_when_hit",
    "entity_action": {
      "type": "origins:chance",
      "chance": 0.2,
      "action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:execute_command",
            "command": "execute at @s run particle minecraft:sculk_soul ~ ~0.5 ~ 0.5 0.5 0.5 0.03 5 force"
          }
        ]
      }
    }
  },
  "magic_damage": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "operation": "multiply_total",
      "value": 3
    },
    "damage_condition": {
      "type": "apugli:magic"
    }
  },
  "fire_damage": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "operation": "multiply_total",
      "value": 3
    },
    "damage_condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:type",
          "damage_type": "onFire"
        },
        {
          "type": "origins:type",
          "damage_type": "onFire.player"
        },
        {
          "type": "origins:type",
          "damage_type": "inFire"
        },
        {
          "type": "origins:type",
          "damage_type": "inFire.player"
        }
      ]
    }
  },
  "kill_particle": {
    "type": "origins:action_on_hit",
    "bientity_action": {
      "type": "origins:target_action",
      "action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:execute_command",
            "command": "particle minecraft:sculk_soul ~ ~0.5 ~ 0.5 0.5 0.5 0.03 5 force"
          }
        ]
      }
    },
    "bientity_condition": {
      "type": "origins:target_condition",
      "condition": {
        "type": "origins:health",
        "compare_to": 1,
        "comparison": "<"
      }
    }
  },
  "gain_literally_anything_else": {
    "type": "origins:self_action_on_kill",
    "entity_action": {
      "type": "origins:change_resource",
      "change": 100,
      "operation": "add",
      "resource": "*:*_soul_bar"
    },
    "target_condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:player",
          "inverted": true
        },
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:wither",
          "inverted": true
        },
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:warden",
          "inverted": true
        },
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:ender_dragon",
          "inverted": true
        }
      ]
    }
  },
  "gain_wither": {
    "type": "origins:self_action_on_kill",
    "entity_action": {
      "type": "origins:change_resource",
      "change": 250,
      "operation": "add",
      "resource": "*:*_soul_bar"
    },
    "target_condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:wither"
        }
      ]
    }
  },
  "gain_ender_dragon": {
    "type": "origins:self_action_on_kill",
    "entity_action": {
      "type": "origins:change_resource",
      "change": 1000,
      "operation": "add",
      "resource": "*:*_soul_bar"
    },
    "target_condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:ender_dragon"
        }
      ]
    }
  },
  "gain_warden": {
    "type": "origins:self_action_on_kill",
    "entity_action": {
      "type": "origins:change_resource",
      "change": 300,
      "operation": "add",
      "resource": "*:*_soul_bar"
    },
    "target_condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:warden"
        }
      ]
    }
  },
  "gain_player": {
    "type": "origins:self_action_on_kill",
    "entity_action": {
      "type": "origins:change_resource",
      "change": 100,
      "operation": "add",
      "resource": "*:*_soul_bar"
    },
    "target_condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:player"
        }
      ]
    }
  },
  "no_vampire_fail_safe": {
    "type": "origins:action_on_callback",
    "execute_chosen_when_orb": true,
    "entity_action_added": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "vampirism level none @s"
        },
        {
          "type": "origins:execute_command",
          "command": "/title @s actionbar {\"text\":\"The Spirits disspell the dark magic in you.\",\"color\":\"dark_blue\",\"bold\":true}"
        }
      ]
    },
    "entity_action_gained": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "vampirism level none @s"
        },
        {
          "type": "origins:execute_command",
          "command": "/title @s actionbar {\"text\":\"The Spirits disspell the dark magic in you.\",\"color\":\"dark_blue\",\"bold\":true}"
        }
      ]
    },
    "entity_action_chosen": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "vampirism level none @s"
        },
        {
          "type": "origins:execute_command",
          "command": "/title @s actionbar {\"text\":\"The Spirits disspell the dark magic in you.\",\"color\":\"dark_blue\",\"bold\":true}"
        }
      ]
    },
    "entity_action_respawned": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "vampirism level none @s"
        },
        {
          "type": "origins:execute_command",
          "command": "/title @s actionbar {\"text\":\"The Spirits disspell the dark magic in you.\",\"color\":\"dark_blue\",\"bold\":true}"
        }
      ]
    }
