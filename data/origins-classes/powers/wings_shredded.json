{
  "type": "origins:multiple",
  "shreded_wings": {
    "type": "origins:prevent_elytra_flight"
  },
  "if_creative_flight": {
    "type": "origins:action_over_time",
    "interval": 5,
    "entity_action": {
      "type": "origins:add_velocity",
      "y": -5,
      "space": "local_horizontal_normalized"
    },
    "condition": {
      "type": "origins:creative_flying"
    }
  },
  "remove": {
    "type": "origins:action_over_time",
    "interval": 200,
    "entity_action": {
      "type": "origins:revoke_power",
      "power": "origins-classes:wings_shredded",
      "source": "origins-classes:wingshredder"
    }
  },
  "if_entity": {
    "type": "origins:action_over_time",
    "interval": 5,
    "entity_action": {
      "type": "origins:add_velocity",
      "y": -5,
      "space": "local_horizontal_normalized"
    },
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:phantom"
        },
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:bee"
        },
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:vex"
        },
        {
          "type": "origins:entity_type",
          "entity_type": "minecraft:parrot"
        },
        {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:entity_type",
              "entity_type": "minecraft:ender_dragon",
              "inverted": true
            },
            {
              "type": "origins:entity_type",
              "entity_type": "minecraft:wither",
              "inverted": true
            }
          ]
        }
      ]
    }
  }
}
