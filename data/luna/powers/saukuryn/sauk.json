{
  "name": "Giant Beast",
  "description": "You are much larger than other critters.\nYou have wings, allowing you to fly!\nBeing made of wood on some parts of your body, you burn much easier.",
  "type": "origins:multiple",
  "height": {
    "type": "origins:action_on_callback",
    "execute_chosen_when_orb": true,
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1.5 @s"
    },
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1.5 @s"
    },
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1.5 @s"
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1 @s"
    },
    "entity_action_removed": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1 @s"
    }
  },
  "width": {
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1.5 @s"
    },
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1.5 @s"
    },
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1.5 @s"
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1 @s"
    },
    "entity_action_removed": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1 @s"
    }
  },
  "elytra": {
    "type": "origins:elytra_flight",
    "render_elytra": true,
    "texture_location": "minecraft:textures/block/cherry_leaves.png"
  },
  "berries": {
    "type": "origins:modify_food",
    "item_condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:ingredient",
          "ingredient": {
            "item": "minecraft:sweet_berries"
          }
        },
        {
          "type": "origins:ingredient",
          "ingredient": {
            "item": "minecraft:glow_berries"
          }
        }
      ]
    },
    "food_modifier": {
      "operation": "addition",
      "value": 8
    },
    "saturation_modifier": {
      "operation": "addition",
      "value": 0.8
    }
  },
  "more_food": {
    "type": "origins:modify_exhaustion",
    "modifier": {
      "value": 1,
      "operation": "multiply_total"
    }
  },
  "fire": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "value": 1.5,
      "operation": "multiply_total"
    },
    "damage_condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:name",
          "name": "minecraft:in_fire"
        },
        {
          "type": "origins:name",
          "name": "minecraft:on_fire"
        },
        {
          "type": "origins:name",
          "name": "minecraft:lava"
        },
        {
          "type": "origins:name",
          "name": "minecraft:campfire"
        }
      ]
    }
  },
  "messages": {
    "type": "origins:self_action_on_hit",
    "entity_action": {
      "type": "origins:chance",
      "chance": 0.3,
      "action": {
        "type": "origins:choice",
        "actions": [
          {
            "element": {
              "type": "origins:execute_command",
              "command": "tellraw @s {\"text\":\"<Ceravol> Stop hurting them!\"}"
            },
            "weight": 1
          },
          {
            "element": {
              "type": "origins:execute_command",
              "command": "tellraw @s {\"text\":\"<Ceravol> WHAT ARE YOU DOING?\"}"
            },
            "weight": 1
          },
          {
            "element": {
              "type": "origins:execute_command",
              "command": "tellraw @s {\"text\":\"<Ceravol> This isn't the way!\"}"
            },
            "weight": 1
          },
          {
            "element": {
              "type": "origins:execute_command",
              "command": "tellraw @s {\"text\":\"<Ceravol> Don't do this!\"}"
            },
            "weight": 1
          },
          {
            "element": {
              "type": "origins:execute_command",
              "command": "tellraw @s {\"text\":\"<Ceravol> We protect the critters not hurt them!\"}"
            },
            "weight": 1
          },
          {
            "element": {
              "type": "origins:execute_command",
              "command": "tellraw @s {\"text\":\"<Ceravol> Life finds a way, except you aren't listening!\"}"
            },
            "weight": 1
          }
        ]
      }
    }
  },
  "jump": {
    "type": "origins:modify_jump",
    "modifier": {
      "value": 0.1,
      "operation": "addition"
    }
  },
  "fall": {
    "type": "origins:modify_falling",
    "take_fall_damage": true,
    "velocity": 0.04
  },
  "flying": {
    "type": "origins:action_over_time",
    "interval": 60,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:add_velocity",
          "z": 0.5,
          "space": "local"
        },
        {
          "type": "origins:execute_command",
          "command": "playsound minecraft:entity.ender_dragon.flap master @s ~ ~ ~ 100 .5"
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:fall_flying"
        },
        {
          "type": "origins:power_active",
          "power": "*:*_toggle"
        }
      ]
    }
  },
  "toggle": {
    "type": "origins:toggle",
    "active_by_default": true,
    "key": "key.saveToolbarActivator"
  },
  "hit_reduction": {
    "type": "origins:modify_damage_dealt",
    "modifier": {
      "value": -0.5,
      "operation": "multiply_total"
    }
  },
  "reach": {
    "type": "origins:attribute",
    "modifiers": [
      {
        "attribute": "reach-entity-attributes:attack_range",
        "value": 1,
        "operation": "addition"
      },
      {
        "attribute": "reach-entity-attributes:reach",
        "value": 2,
        "operation": "addition"
      }
    ]
  },
  "particle": {
    "type": "origins:action_over_time",
    "interval": 10,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "particle minecraft:dragon_breath ~ ~1.5 ~ 0.5 0.7 0.5 0.01 10 force"
    }
  }
}
