{
  "type": "origins:multiple",
  "name": "Dragonkin",
  "description": "You are a dragon, You have the abilitys of a normal dragon.\nYou take more damage from arrows.\nYou attract more lightning!",
  "elytra": {
    "type": "origins:elytra_flight",
    "render_elytra": true,
    "condition": {
      "type": "origins:submerged_in",
      "fluid": "minecraft:water",
      "inverted": true
    }
  },
  "immune_to_velocity_damage": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "value": -1,
      "operation": "multiply_total"
    },
    "damage_condition": {
      "type": "origins:type",
      "damage_type": "flyIntoWall"
    }
  },
  "storm_power": {
    "type": "origins:action_over_time",
    "interval": 10,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:apply_effect",
          "effects": [
            {
              "effect": "speed",
              "amplifier": 1,
              "duration": 40,
              "show_particles": false,
              "show_icon": true
            },
            {
              "effect": "resistance",
              "amplifier": 1,
              "duration": 40,
              "show_particles": false,
              "show_icon": true
            }
          ]
        }
      ]
    },
    "condition": {
      "type": "origins:predicate",
      "predicate": "luna:is_thundering"
    }
  },
  "lightning_no_hurt": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "value": -1,
      "operation": "multiply_total"
    },
    "damage_condition": {
      "type": "origins:name",
      "name": "lightningBolt"
    }
  },
  "more_arrow_damage": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "value": 0.5,
      "operation": "multiply_total"
    },
    "damage_condition": {
      "type": "origins:projectile",
      "projectile": "minecraft:arrow"
    }
  },
  "height": {
    "type": "origins:action_on_callback",
    "execute_chosen_when_orb": true,
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1.3 @s",
      "permission_level": 4
    },
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1.3 @s",
      "permission_level": 4
    },
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1.3 @s",
      "permission_level": 4
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1 @s",
      "permission_level": 4
    },
    "entity_action_removed": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1 @s",
      "permission_level": 4
    }
  },
  "width": {
    "type": "origins:action_on_callback",
    "execute_chosen_when_orb": true,
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1.3 @s",
      "permission_level": 4
    },
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1.3 @s",
      "permission_level": 4
    },
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1.3 @s",
      "permission_level": 4
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1 @s",
      "permission_level": 4
    },
    "entity_action_removed": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1 @s",
      "permission_level": 4
    }
  },
  "hurt_sound": {
    "type": "origins:action_when_damage_taken",
    "cooldown": 0,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "playsound minecraft:entity.ender_dragon.hurt master @s ~ ~ ~ 100 .5"
    }
  },
  "lightning_rod": {
    "type": "apugli:redirect_lightning",
    "chance": 0.8
  }
}
