              }
            ]
          },
          "condition": {
            "type": "origins:equipped_item",
            "equipment_slot": "mainhand",
            "item_condition": {
              "type": "origins:ingredient",
              "ingredient": {
                "item": "minecraft:cobbled_deepslate"
              }
            }
          }
        }
      ]
    },
    "condition": {
      "type": "origins:resource",
      "compare_to": 1000,
      "comparison": "!=",
      "resource": "*:wisps/bars_blue_wisp_bar"
    }
  },
  "falling": {
