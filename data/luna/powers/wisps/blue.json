                "resource": "*:wisps/bars_blue_wisp_bar"
              },
              {
                "type": "origins:execute_command",
                "command": "clear @s minecraft:stone 1"
              }
            ]
          },
          "condition": {
            "type": "origins:equipped_item",
            "equipment_slot": "mainhand",
            "item_condition": {
