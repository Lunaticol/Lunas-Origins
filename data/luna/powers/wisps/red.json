{
  "name": "Red Wisp",
  "description": "Red Wisps are firey and explosive. They love to hang out in the nether & love sweet treats!",
  "type": "origins:multiple",
  "gain": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:change_resource",
      "change": 20,
      "resource": "*:wisps/bars_red_wisp_bar"
    },
    "condition": {
      "type": "origins:dimension",
      "dimension": "minecraft:the_nether"
    }
  },
  "burst": {
    "type": "origins:active_self",
    "cooldown": 200,
    "hud_render": {
      "should_render": true,
      "sprite_location": "origins:textures/gui/luna_resource_bar.png",
      "bar_index": 14,
      "order": 7
    },
    "key": "key.origins.secondary_active",
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:area_of_effect",
          "radius": 6,
          "shape": "sphere",
          "bientity_action": {
            "type": "origins:target_action",
            "action": {
              "type": "origins:and",
              "actions": [
                {
                  "type": "origins:damage",
                  "amount": 10,
                  "source": {
                    "fire": true,
                    "unblockable": true,
                    "name": "explosion"
                  }
                },
                {
                  "type": "origins:set_on_fire",
                  "duration": 7
                },
                {
                  "type": "origins:grant_power",
                  "power": "luna:wisps/flashbang",
                  "source": "*:wisps/red"
                }
              ]
            }
          }
        },
        {
          "type": "origins:play_sound",
          "category": "players",
          "pitch": 0.5,
          "sound": "minecraft:entity.generic.explode",
          "volume": 1
        },
        {
          "type": "origins:execute_command",
          "command": "particle lunasorigins:red_flame ~ ~1 ~ 0.5 0.5 0.8 0.5 250"
        },
        {
          "type": "origins:execute_command",
          "command": "particle minecraft:flash ~ ~1 ~ 0 0 0 1 10"
        },
        {
          "type": "origins:change_resource",
          "change": -50,
          "resource": "*:wisps/bars_red_wisp_bar"
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "compare_to": 1,
          "comparison": "==",
          "resource": "*:wisps/wisp_switch_resource"
        },
        {
          "type": "origins:resource",
          "compare_to": 50,
          "comparison": ">=",
          "resource": "*:wisps/bars_red_wisp_bar"
        },
        {
          "type": "origins:resource",
          "compare_to": 0,
          "comparison": "==",
          "resource": "*:*_ult_active"
        }
      ]
    }
  },
  "gain_sugars": {
