      "type": "origins:chance",
      "chance": 0.2,
      "action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:execute_command",
            "command": "execute at @s run particle minecraft:sculk_soul ~ ~0.5 ~ 0.5 0.5 0.5 0.03 5 force"
          }
        ]
      }
    }
  },
  "magic_damage": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "operation": "multiply_total",
      "value": 1.5
    },
    "damage_condition": {
      "type": "apugli:magic"
    }
  },
  "fire_damage": {
