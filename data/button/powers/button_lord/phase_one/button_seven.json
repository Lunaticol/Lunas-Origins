{
  "type": "origins:multiple",
  "button_rooms": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:delay",
          "ticks": 1,
          "action": {
            "type": "origins:execute_command",
            "command": "tp @a[distance-..200] ~ ~ ~"
          }
        },
        {
          "type": "origins:delay",
          "ticks": 2,
          "action": {
            "type": "origins:execute_command",
            "command": "tp @s[distance-..200] ~ ~ ~"
          }
        }
      ]
    }
  }
}
