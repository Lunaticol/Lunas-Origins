{
  "type": "origins:multiple",
  "name": "Good Padding",
  "description": "You have a padding on your feet, allowing you to move faster and soften your falls. It is flammable, though.",
  "badges": [
    {
      "type": "origins:tooltip",
      "sprite": "origins:textures/gui/badge/info.png",
      "text": "You are quicker on foot. You take no fall damage. You take more fire damage."
    }
  ],
  "faster_base_speed_respawn": {
    "type": "origins:action_on_callback",
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "attribute @s minecraft:generic.movement_speed base set 0.125"
    }
  },
  "faster_base_speed_chosen": {
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "attribute @s minecraft:generic.movement_speed base set 0.125"
    }
  },
  "normal_base_speed": {
    "type": "origins:action_on_callback",
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "attribute @s minecraft:generic.movement_speed base set 0.1"
    }
  },
  "no_fall_dmg": {
    "type": "origins:modify_damage_taken",
    "damage_condition": {
      "type": "origins:from_falling"
    },
    "modifier": {
      "operation": "set_base",
      "value": 0
    }
  },
  "more_fire_damage": {
    "type": "origins:modify_damage_taken",
    "damage_condition": {
      "type": ""
    },
    "modifier": {
      "operation": "multiply_base",
      "value": 1
    }
  },
  "bigger_base_jumps_chosen": {
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:jump_height 1.4"
    }
  }
}
