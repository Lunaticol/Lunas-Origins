{
  "name": "Axolotl Body",
  "description": "Youre an Axolotl! You are smaller and have water buffs!",
  "type": "origins:multiple",
  "night_vision": {
    "type": "origins:night_vision",
    "strength": 1,
    "condition": {
      "type": "origins:submerged_in",
      "fluid": "minecraft:water"
    }
  },
  "health": {
    "type": "origins:attribute",
    "modifier": {
      "attribute": "minecraft:generic.max_health",
      "value": -4,
      "operation": "addition"
    }
  },
  "breathe_in_water": {
    "type": "origins:action_over_time",
    "interval": 1,
    "conditions": [
      {
        "type": "origins:fluid_height",
        "fluid": "minecraft:water",
        "comparison": ">",
        "compare_to": 0.1
      }
    ],
    "entity_action": {
      "type": "origins:gain_air",
      "value": 20
    }
  },
  "size": {
    "type": "origins:action_on_callback",
    "execute_chosen_when_orb": true,
    "entity_action_chosen": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "scale set pehkui:hieght 0.5 @s"
        },
        {
          "type": "origins:grant_advancement",
          "advancement": "luna:axolotl/axolotl"
        }
      ]
    },
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 0.5 @s"
    },
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 0.5 @s"
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1 @s"
    },
    "entity_action_removed": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:height 1 @s"
    }
  },
  "width": {
    "type": "origins:action_on_callback",
    "execute_chosen_when_orb": true,
    "entity_action_chosen": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 0.5 @s"
    },
    "entity_action_respawned": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 0.5 @s"
    },
    "entity_action_added": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 0.5 @s"
    },
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1 @s"
    },
    "entity_action_removed": {
      "type": "origins:execute_command",
      "command": "scale set pehkui:width 1 @s"
    }
  },
  "lush_regen_in_the_lush_caves_biome": {
    "type": "origins:action_over_time",
    "interval": 1,
    "condition": {
      "type": "origins:biome",
      "biome": "minecraft:lush_caves"
    },
    "entity_action": {
      "type": "origins:apply_effect",
      "effects": [
        {
          "effect": "regeneration",
          "duration": 2,
          "show_particles": false,
          "is_ambient": false
        }
      ]
    }
  },
  "axolotl_hurt": {
    "type": "origins:action_when_damage_taken",
    "cooldown": 0,
    "entity_action": {
      "type": "origins:play_sound",
      "sound": "minecraft:entity.axolotl.hurt",
      "category": "players",
      "pitch": 1,
      "volume": 1
    }
  },
  "sounds": {
    "type": "origins:action_over_time",
    "interval": 100,
    "entity_action": {
      "type": "origins:chance",
      "chance": 0.5,
      "action": {
        "type": "origins:play_sound",
        "sound": "minecraft:entity.axolotl.idle_water",
        "category": "players",
        "pitch": 1,
        "volume": 1
      }
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": 0.1
        },
        {
          "type": "origins:sneaking",
          "inverted": true
        }
      ]
    }
  },
  "sounds_air": {
    "type": "origins:action_over_time",
    "interval": 100,
    "entity_action": {
      "type": "origins:chance",
      "chance": 0.5,
      "action": {
        "type": "origins:play_sound",
        "sound": "minecraft:entity.axolotl.idle_air",
        "category": "players",
        "pitch": 1,
        "volume": 1
      }
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": 0.09,
          "inverted": true
        },
        {
          "type": "origins:sneaking",
          "inverted": true
        }
      ]
    }
  },
  "entity_type": {
    "type": "origins:entity_group",
    "group": "aquatic"
  },
  "slippery": {
    "type": "origins:modify_slipperiness",
    "modifier": {
      "name": "slip",
      "value": 0.7,
      "operation": "multiply_total"
    },
    "condition": {
      "inverted": true,
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:block_in_radius",
          "compare_to": 1,
          "comparison": ">=",
          "radius": 1,
          "shape": "cube",
          "block_condition": {
            "type": "origins:or",
            "conditions": [
              {
                "type": "origins:in_tag",
                "tag": "chipped:ice"
              },
              {
                "type": "origins:in_tag",
                "tag": "chipped:packed_ice"
              },
              {
                "type": "origins:in_tag",
                "tag": "chipped:blue_ice"
              }
            ]
          }
        },
        {
          "type": "origins:fluid_height",
          "fluid": "minecraft:water",
          "comparison": ">",
          "compare_to": 0.09
        },
        {
          "type": "origins:enchantment",
          "enchantment": "moborigins:ground_spikes",
          "calculation": "sum",
          "compare_to": 1,
          "comparison": "=="
        }
      ]
    }
  },
  "change_origin": {
    "type": "origins:action_on_callback",
    "entity_action_lost": {
      "type": "origins:execute_command",
      "command": "origin set @s axolotl:axolotl_subclass axolotl:axolotl_classes/empty"
    }
  }
}
