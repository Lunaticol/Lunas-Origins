{
  "name": "Deep and Dark",
  "description": "Born from Sculk, and kin to the Warden. Go froward, and show these invaders why they shouldnt mess with the Sculk.",
  "type": "origins:multiple",
  "city": {
    "type": "origins:modify_player_spawn",
    "structure": "minecraft:ancient_city",
    "spawn_strategy": "center",
    "dimension": "minecraft:overworld"
  },
  "sculk_speed": {
    "type": "origins:conditioned_attribute",
    "modifiers": [
      {
        "operation": "multiply_total",
        "value": 0.5,
        "attribute": "minecraft:generic.movement_speed"
      },
      {
        "operation": "multiply_total",
        "value": 1,
        "attribute": "minecraft:generic.attack_speed"
      }
    ],
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:on_block",
          "block_condition": {
            "type": "origins:in_tag",
            "tag": "sculk_luna:sculk_blocks"
          }
        },
        {
          "type": "origins:in_block",
          "block_condition": {
            "type": "origins:in_tag",
            "tag": "sculk_luna:sculk_blocks"
          }
        }
      ]
    }
  },
  "xp_on_cat": {
    "type": "origins:modify_xp_gain",
    "modifier": {
      "value": 5,
      "operation": "multiply_total"
    },
    "condition": {
      "type": "origins:on_block",
      "block_condition": {
        "type": "origins:block",
        "block": "minecraft:sculk_catalyst"
      }
    }
  },
  "xp_off_cat": {
    "type": "origins:modify_xp_gain",
    "modifier": {
      "value": 2.5,
      "operation": "multiply_total"
    },
    "condition": {
      "inverted": true,
      "type": "origins:on_block",
      "block_condition": {
        "type": "origins:block",
        "block": "minecraft:sculk_catalyst"
      }
    }
  },
  "this_block_is_cool": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:apply_effect",
          "effect": {
            "effect": "minecraft:regeneration",
            "amplifier": 1,
            "duration": 40
          }
        }
      ]
    },
    "condition": {
      "type": "origins:block_in_radius",
      "radius": 8,
      "block_condition": {
        "type": "origins:block",
        "block": "minecraft:sculk_catalyst"
      }
    }
  },
  "shader": {
    "type": "origins:shader",
    "shader": "minecraft:shaders/post/sobel.json",
    "toggleable": false
  },
  "condition": {
    "type": "origins:block_in_radius",
    "radius": 8,
    "block_condition": {
      "type": "origins:in_tag",
      "tag": "sculk_luna:sculk_blocks"
    }
  },
  "vibrations": {
    "type": "origins:game_event_listener",
    "entity": true,
    "trigger_order": "by_distance",
    "show_particle": true,
    "event_tag": "minecraft:vibrations",
    "cooldown": 0,
    "bientity_action": {
      "type": "origins:actor_action",
      "action": {
        "type": "origins:and",
        "actions": [
          {
            "type": "origins:grant_power",
            "power": "*:detected",
            "source": "sculk_luna:sculk_like"
          }
        ]
      }
    },
    "bientity_condition": {
      "type": "origins:actor_condition",
      "condition": {
        "type": "origins:exists"
      }
    }
  },
  "mob_no_see": {
    "type": "origins:prevent_entity_render",
    "entity_condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:exists"
        },
        {
          "type": "origins:power",
          "power": "sculk_luna:detected",
          "inverted": true
        },
        {
          "type": "origins:in_tag",
          "tag": "sculk_luna:can_always_see",
          "inverted": true
        }
      ]
    },
    "bientity_condition": {
      "type": "origins:distance",
      "compare_to": 2,
      "comparison": ">="
    }
  },
  "if_detected": {
    "type": "origins:entity_glow",
    "red": 0.1,
    "green": 0.8,
    "blue": 0.6,
    "use_teams": false,
    "entity_condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:exists"
        },
        {
          "type": "origins:power",
          "power": "sculk_luna:detected"
        }
      ]
    }
  },
  "fire": {
    "type": "origins:action_over_time",
    "interval": 40,
    "entity_action": {
      "type": "origins:add_xp",
      "levels": -1
    },
    "condition": {
      "type": "origins:on_fire"
    }
  },
  "fire_hurty": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "value": 4,
      "operation": "multiply_total"
    },
    "damage_condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:name",
          "name": "minecraft:in_fire"
        },
        {
          "type": "origins:name",
          "name": "minecraft:on_fire"
        }
      ]
    }
  },
  "immune": {
    "type": "origins:effect_immunity",
    "effects": ["minecraft:darkness", "minecraft:blindness"]
  },
  "detect_if_hit": {
    "type": "origins:action_on_hit",
    "bientity_action": {
      "type": "origins:target_action",
      "action": {
        "type": "origins:grant_power",
        "power": "*:detected",
        "source": "sculk_luna:sculk"
      }
    }
  },
  "if_on_sculk": {
    "type": "origins:action_over_time",
    "interval": 10,
    "entity_action": {
      "type": "origins:area_of_effect",
      "radius": 32,
      "shape": "sphere",
      "bientity_action": {
        "type": "origins:target_action",
        "action": {
          "type": "origins:grant_power",
          "power": "*:detected",
          "source": "sculk_luna:sculk_like"
        }
      }
    },
    "condition": {
      "type": "origins:on_block",
      "block_condition": {
        "type": "origins:in_tag",
        "tag": "sculk_luna:sculk_blocks"
      }
    }
  },
  "silent": {
    "type": "origins:prevent_game_event",
    "tag": "minecraft:vibrations"
  },
  "to_much_xp": {
    "type": "origins:action_over_time",
    "interval": 40,
    "entity_action": {
      "type": "origins:add_xp",
      "levels": -1
    },
    "condition": {
      "type": "origins:xp_levels",
      "compare_to": 200,
      "comparison": ">"
    }
  }
}
