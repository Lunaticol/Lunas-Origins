{
  "type": "origins:multiple",
  "hidden": true,
  "overworld_default": {
    "type": "origins:action_on_callback",
    "execute_chosen_when_orb": true,
    "entity_action_respawned": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "function luna:kitsune/respawn"
        },
        {
          "type": "origins:change_resource",
          "change": 1,
          "operation": "set",
          "resource": "luna:kitsune/mode_switch_mode_resource"
        }
      ]
    },
    "entity_action_added": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "function luna:kitsune/respawn"
        },
        {
          "type": "origins:change_resource",
          "change": 1,
          "operation": "set",
          "resource": "luna:kitsune/mode_switch_mode_resource"
        }
      ]
    },
    "entity_action_chosen": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "function luna:kitsune/overworld_mode"
        },
        {
          "type": "origins:change_resource",
          "change": 1,
          "operation": "set",
          "resource": "luna:kitsune/mode_switch_mode_resource"
        },
        {
          "type": "origins:grant_advancement",
          "advancement": "luna:kitsune/kitsune"
        }
      ]
    },
    "entity_action_lost": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "origin set @s luna:luna_layers origins:empty"
        }
      ]
    },
    "entity_action_removed": {
      "type": "origins:execute_command",
      "command": "origin set @s luna:luna_layers origins:empty"
    }
  },
  "xp": {
    "type": "origins:modify_xp_gain",
    "modifier": {
      "operation": "multiply_base",
      "value": 1.5
    }
  },
  "sounds": {
    "type": "origins:action_over_time",
    "interval": 100,
    "entity_action": {
      "type": "origins:chance",
      "chance": 0.5,
      "action": {
        "type": "origins:play_sound",
        "sound": "minecraft:entity.fox.ambient",
        "category": "ambient",
        "pitch": 1,
        "volume": 1
      }
    },
    "condition": {
      "type": "origins:sneaking",
      "inverted": true
    }
  },
  "fox_hurt": {
    "type": "origins:action_when_damage_taken",
    "cooldown": 0,
    "entity_action": {
      "type": "origins:play_sound",
      "sound": "minecraft:entity.fox.hurt",
      "category": "ambient",
      "pitch": 1,
      "volume": 1
    }
  },
  "no_taming_wolfs": {
    "type": "origins:prevent_entity_use",
    "bientity_action": {
      "type": "origins:nothing"
    },
    "bientity_condition": {
      "type": "origins:target_condition",
      "condition": {
        "type": "origins:entity_type",
        "entity_type": "minecraft:wolf"
      }
    }
  },
  "sweet_berries": {
    "type": "origins:modify_food",
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": {
        "item": "minecraft:sweet_berries"
      }
    },
    "food_modifier": {
      "operation": "addition",
      "value": 7
    },
    "saturation_modifier": {
      "operation": "addition",
      "value": 0.7
    }
  },
  "wolf_scared": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:apply_effect",
      "effect": {
        "effect": "minecraft:weakness",
        "amplifier": 3,
        "duration": 40
      }
    },
    "condition": {
      "type": "apugli:entity_in_radius",
      "compare_to": 1,
      "comparison": ">=",
      "radius": 3,
      "condition": {
        "type": "origins:entity_type",
        "entity_type": "minecraft:wolf"
      }
    }
  },
  "origin_counter": {
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "scoreboard players add DimensionalKitsune Origin 1"
        }
      ]
    },
    "entity_action_lost": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "scoreboard players remove DimensionalKitsune Origin 1"
        }
      ]
    }
  },
  "dimensional_berries": {
    "type": "origins:modify_food",
    "item_condition": {
      "type": "origins:ingredient",
      "ingredient": {
        "item": "lunasorigins:dimensional_berry"
      }
    },
    "food_modifier": {
      "operation": "addition",
      "value": 15
    },
    "saturation_modifier": {
      "operation": "addition",
      "value": 1.5
    }
  }
}
