          {
            "type": "origins:execute_command",
            "command": "particle minecraft:sculk_soul ~ ~0.5 ~ 0.5 0.5 0.5 0.03 5 force"
          }
        ]
      }
    },
    "bientity_condition": {
      "type": "origins:target_condition",
      "condition": {
        "type": "origins:health",
        "compare_to": 1,
        "comparison": "<"
      }
    }
  },
