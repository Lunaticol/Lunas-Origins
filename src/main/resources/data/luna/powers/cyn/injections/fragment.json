{
  "type": "origins:multiple",
  "teleport": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:chance",
      "chance": 0.75,
      "action": {
        "type": "origins:execute_command",
        "command": "function luna:cyn/teleport"
      }
    }
  },
  "remove": {
    "type": "origins:action_over_time",
    "interval": 150,
    "entity_action": {
      "type": "origins:revoke_power",
      "power": "luna:cyn/injections/fragment",
      "source": "luna:cyn/inject"
    }
  }
}
