{
  "name": "Void Affinity",
  "description": "Manipulate gravity to slow down enemies in a small radius around you.\nCauses projecitles to warp around you.",
  "type": "origins:multiple",
  "gravity": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "origins:area_of_effect",
      "radius": 8,
      "shape": "sphere",
      "bientity_action": {
        "type": "origins:add_velocity",
        "y": 0.2,
        "z": 0.1
      },
      "bientity_condition": {
        "type": "origins:target_condition",
        "condition": {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:or",
              "conditions": [
                {
                  "type": "origins:block_collision",
                  "offset_x": 0.1,
                  "offset_y": 0.1,
                  "offset_z": 0.1,
                  "inverted": true
                },
                {
                  "type": "origins:block_collision",
                  "offset_x": -0.1,
                  "offset_y": -0.1,
                  "offset_z": -0.1,
                  "inverted": true
                }
              ]
            },
            {
              "type": "origins:or",
              "conditions": [
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:arrow"
                },
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:trident"
                },
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:spectral_arrow"
                },
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:potion"
                }
              ]
            }
          ]
        }
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    }
  },
  "gravity_small_things": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "origins:area_of_effect",
      "radius": 8,
      "shape": "sphere",
      "bientity_action": {
        "type": "origins:add_velocity",
        "y": 0.2,
        "z": 0.1
      },
      "bientity_condition": {
        "type": "origins:target_condition",
        "condition": {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:or",
              "conditions": [
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:arrow"
                },
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:trident"
                },
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:spectral_arrow"
                },
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:potion"
                },
                {
                  "type": "origins:command",
                  "command": "scale get pehkui:base @s 10",
                  "comparison": ">",
                  "compare_to": 5
                },
                {
                  "type": "origins:command",
                  "command": "scale get pehkui:height @s 10",
                  "comparison": ">",
                  "compare_to": 5
                },
                {
                  "type": "origins:command",
                  "command": "scale get pehkui:width @s 10",
                  "comparison": ">",
                  "compare_to": 5
                },
                {
                  "type": "origins:command",
                  "command": "scale get @s 10",
                  "comparison": ">",
                  "compare_to": 5
                }
              ]
            }
          ]
        }
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    }
  },
  "particle": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "origins:area_of_effect",
      "radius": 8,
      "shape": "sphere",
      "bientity_action": {
        "type": "origins:target_action",
        "action": {
          "type": "origins:execute_command",
          "command": "particle dust_color_transition 0.000 0.000 0.000 2 1.000 0.000 0.969 ~ ~1 ~ 0.1 0.1 0.1 1 5 normal"
        }
      },
      "bientity_condition": {
        "type": "origins:target_condition",
        "condition": {
          "type": "origins:and",
          "conditions": [
            {
              "type": "origins:or",
              "conditions": [
                {
                  "type": "origins:block_collision",
                  "offset_x": 0.1,
                  "offset_y": 0.1,
                  "offset_z": 0.1,
                  "inverted": true
                },
                {
                  "type": "origins:block_collision",
                  "offset_x": -0.1,
                  "offset_y": -0.1,
                  "offset_z": -0.1,
                  "inverted": true
                }
              ]
            },
            {
              "type": "origins:or",
              "conditions": [
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:arrow"
                },
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:trident"
                },
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:spectral_arrow"
                },
                {
                  "type": "origins:entity_type",
                  "entity_type": "minecraft:potion"
                }
              ]
            }
          ]
        }
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    }
  },
  "particle_entity": {
    "type": "origins:action_over_time",
    "interval": 1,
    "entity_action": {
      "type": "origins:area_of_effect",
      "radius": 8,
      "shape": "sphere",
      "bientity_action": {
        "type": "origins:target_action",
        "action": {
          "type": "origins:execute_command",
          "command": "particle dust_color_transition 0.000 0.000 0.000 2 1.000 0.000 0.969 ~ ~1 ~ 0.1 0.1 0.1 1 5 normal"
        }
      },
      "bientity_condition": {
        "type": "origins:target_condition",
        "condition": {
          "type": "origins:or",
          "conditions": [
            {
              "type": "origins:command",
              "command": "scale get pehkui:base @s 10",
              "comparison": ">",
              "compare_to": 5
            },
            {
              "type": "origins:command",
              "command": "scale get pehkui:height @s 10",
              "comparison": ">",
              "compare_to": 5
            },
            {
              "type": "origins:command",
              "command": "scale get pehkui:width @s 10",
              "comparison": ">",
              "compare_to": 5
            },
            {
              "type": "origins:command",
              "command": "scale get @s 10",
              "comparison": ">",
              "compare_to": 5
            }
          ]
        }
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    }
  },
  "toggle": {
    "type": "origins:toggle",
    "active_by_default": false,
    "key": "key.saveToolbarActivator",
    "condition": {
      "type": "origins:resource",
      "compare_to": 1,
      "comparison": ">=",
      "resource": "*:umbryte/void_void_bar"
    }
  },
  "slow_down": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:area_of_effect",
      "radius": 3,
      "bientity_action": {
        "type": "origins:target_action",
        "action": {
          "type": "origins:apply_effect",
          "effect": {
            "effect": "minecraft:slowness",
            "amplifier": 1,
            "duration": 40
          }
        }
      }
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    }
  },
  "active": {
    "type": "origins:conditioned_attribute",
    "modifier": {
      "attribute": "minecraft:generic.movement_speed",
      "value": -0.25,
      "operation": "multiply_total"
    },
    "condition": {
      "type": "origins:power_active",
      "power": "*:*_toggle"
    }
  }
}
