  },
  "hunger_3": {
    "type": "origins:modify_exhaustion",
    "modifier": {
      "value": 4,
      "operation": "multiply_total"
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:resource",
          "compare_to": 200,
          "comparison": ">=",
          "resource": "luna:piglin/zombification_infection_master_bar"
        },
        {
          "type": "origins:resource",
          "compare_to": 300,
          "comparison": "<",
          "resource": "luna:piglin/zombification_infection_master_bar"
        }
