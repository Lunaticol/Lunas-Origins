{
  "name": "Floaty",
  "description": "You're a Skyweaver! You do not touch the ground unless you want to.\nYou can climb surfaces by floating up them\nUsing the air your projectiles hurt more!",
  "type": "origins:multiple",
  "floaty": {
    "type": "origins:action_over_time",
    "interval": 15,
    "entity_action": {
      "type": "origins:apply_effect",
      "effects": [
        {
          "effect": "minecraft:levitation",
          "duration": 13,
          "show_particles": false,
          "show_icon": false
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:block_collision",
          "offset_y": -1
        },
        {
          "type": "origins:sneaking",
          "inverted": true
        }
      ]
    }
  },
  "floaty_blockside": {
    "type": "origins:action_over_time",
    "interval": 15,
    "entity_action": {
      "type": "origins:apply_effect",
      "effects": [
        {
          "effect": "minecraft:levitation",
          "duration": 16,
          "amplifier": 1,
          "show_particles": false,
          "show_icon": false
        }
      ]
    },
    "condition": {
      "type": "origins:and",
      "conditions": [
        {
          "type": "origins:or",
          "conditions": [
            {
              "type": "origins:block_collision",
              "offset_z": -1,
              "offset_x": -1
            },
            {
              "type": "origins:block_collision",
              "offset_z": 1,
              "offset_x": 1
            }
          ]
        },
        {
          "type": "origins:sneaking",
          "inverted": true
        }
      ]
    }
  },
  "perm_slowfalling": {
    "type": "origins:action_over_time",
    "interval": 20,
    "entity_action": {
      "type": "origins:apply_effect",
      "effect": {
        "duration": 20,
        "effect": "minecraft:slow_falling",
        "show_particles": false,
        "show_icon": false
      }
    }
  },
  "really_high_rn": {
    "type": "origins:creative_flight",
    "condition": {
      "type": "origins:in_block",
      "block_condition": {
        "type": "origins:height",
        "compare_to": 185,
        "comparison": ">="
      }
    }
  },
  "bow_experience": {
    "type": "origins:modify_projectile_damage",
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:equipped_item",
          "equipment_slot": "mainhand",
          "item_condition": {
            "type": "origins:ingredient",
            "ingredient": { "item": "minecraft:bow" }
          }
        },
        {
          "type": "origins:equipped_item",
          "equipment_slot": "offhand",
          "item_condition": {
            "type": "origins:empty"
          }
        }
      ]
    },
    "damage_condition": {
      "type": "origins:projectile",
      "projectile": "minecraft:arrow"
    },
    "modifier": {
      "operation": "addition",
      "value": 20.0
    }
  },
  "bow_experience2": {
    "type": "origins:modify_projectile_damage",
    "condition": {
      "type": "origins:or",
      "conditions": [
        {
          "type": "origins:equipped_item",
          "equipment_slot": "mainhand",
          "item_condition": {
            "type": "origins:ingredient",
            "ingredient": { "item": "minecraft:crossbow" }
          }
        },
        {
          "type": "origins:equipped_item",
          "equipment_slot": "offhand",
          "item_condition": {
            "type": "origins:empty"
          }
        }
      ]
    },
    "damage_condition": {
      "type": "origins:projectile",
      "projectile": "minecraft:arrow"
    },
    "modifier": {
      "operation": "addition",
      "value": 8.0
    }
  },
  "knockback": {
    "type": "origins:attribute",
    "modifier": {
      "attribute": "minecraft:generic.knockback_resistance",
      "value": -2,
      "operation": "multiply_total"
    }
  },
  "fire": {
    "type": "origins:modify_damage_taken",
    "modifier": {
      "attribute": "minecraft:generic.knockback_resistance",
      "value": 0.5,
      "operation": "addition"
    },
    "damage_condition": {
      "type": "origins:in_tag",
      "tag": "minecraft:is_fire"
    }
  },
  "advancement": {
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
      "type": "origins:grant_advancement",
      "advancement": "luna:skyweaver/skyweaver"
    }
  },
  "sw": {
    "type": "origins:action_when_damage_taken",
    "cooldown": 0,
    "entity_action": {
      "type": "origins:execute_command",
      "command": "origin set @s origins:origin luna:sw"
    },
    "damage_condition": {
      "type": "origins:name",
      "name": "lightningBolt"
    }
  },
  "miningspeed": {
    "condition": { "type": "origins:on_block", "inverted": true },
    "type": "origins:modify_break_speed",
    "modifier": { "operation": "multiply_total", "value": 3 }
  },
  "origin_counter": {
    "type": "origins:action_on_callback",
    "entity_action_chosen": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "scoreboard players add Skyweaver Origin 1"
        }
      ]
    },
    "entity_action_lost": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:execute_command",
          "command": "scoreboard players remove Skyweaver Origin 1"
        }
      ]
    }
  }
}
