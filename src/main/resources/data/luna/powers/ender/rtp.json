{
  "name": "RTP (Random Teleport)",
  "description": "You are able to randomly teleport.\nYou sometimes teleport when hit.\nProjectiles cannot harm you.\n You can see attackers after theyve hit you.",
  "type": "origins:multiple",
  "badges": [
    {
      "type": "origins:tooltip",
      "sprite": "origins:textures/gui/badge/info.png",
      "text": "This power only works when it is toggled!"
    },
    {
      "type": "origins:keybind",
      "sprite": "origins:textures/gui/badge/toggle.png",
      "text": "Toggle with [Secondary]"
    }
  ],
  "toggle": {
    "type": "origins:toggle",
    "active_by_default": true,
    "key": {
      "key": "key.origins.secondary_active"
    }
  },
  "rtp_when_hit": {
    "type": "origins:modify_damage_taken",
    "bientity_action": {
      "type": "origins:and",
      "actions": [
        {
          "type": "origins:target_action",
          "action": {
            "type": "origins:if_else_list",
            "actions": [
              {
                "action": {
                  "type": "origins:and",
                  "actions": [
                    {
                      "type": "origins:play_sound",
                      "sound": "minecraft:item.chorus_fruit.teleport",
                      "volume": 1,
                      "pitch": 1
                    },
                    {
                      "type": "origins:execute_command",
                      "command": "spreadplayers ~ ~ 8 16 under 127 false @s"
                    },
                    {
                      "type": "origins:execute_command",
                      "command": "particle minecraft:portal ~ ~0.4 ~ 0.3 0.6 0.3 0 100 normal"
                    },
                    {
                      "type": "origins:change_resource",
                      "resource": "*:*_rtp_bar",
                      "change": -1,
                      "operation": "add"
                    },
                    {
                      "type": "origins:trigger_cooldown",
